;!MURBIN

meminit d0 #f"{chr(1)}Hello, world!"
meminit d15 #f"{chr(16)}Good day, computer!"

include std.main
include std.io

world:
    cpy r0 e1 ; move char* to d0 to r0
    cpy r1 d0 ; copy offset lenght to d0 of string to r1
    add r0 r1 ; add onto char*
    putmacro putln ; just puts + print newline
    call computer
    ret

computer:
    cpy r0 e1
    cpy r1 d15
    add r0 r1
    putmacro putln
    ret

main:
    call world
    movz r0
    ret
