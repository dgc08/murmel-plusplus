;!MURBIN
; std.io
$STD.endl: ; endl "\n"
    10
    0

defmacro putchar char:
    movz 4
    cpy 5 char
    syscall
macro_end

putln:
    call puts
    putmacro putchar STD.endl

defmacro println str:
    cpy r0 str
    call putln
macro_end

scope
puts:
    movz 4
    l.start:
        jz *r0 l.finish
        cpy 5 *r0
        inc r0
        syscall
        jmp l.start
    l.finish:
        ret
